{
    "name": "opensoft/rollout",
    "description": "Feature switches or flags for PHP",
    "keywords": ["feature", "flag", "toggle", "rollout", "flipper", "switches"],
    "license": "MIT",
    "authors": [
        {
            "name": "Richard Fullmer",
            "email": "richard.fullmer@opensoftdev.com"
        }
    ],
    "require": {
        "php": ">=5.3.3"
    },
    "suggest": {
        "doctrine/cache": "For use with the DoctrineCacheStorageAdapter",
        "predis/predis": "For use with the RedisStorageAdapter"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "doctrine/cache": "*"
    },
    "autoload": {
        "psr-4": {
            "Opensoft\\Rollout\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
